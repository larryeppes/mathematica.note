     RowBox[{"DivisorSigma", "[", 
      RowBox[{"0", ",", 
       RowBox[{"n", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}]}], "]"}], 
     "\[LessEqual]", "500"}], ",", 
    RowBox[{"n", "++"}]}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"n", 
  RowBox[{
