  RowBox[{
   RowBox[{"{", 
    RowBox[{"pandigitalQ", ",", "pandigitalProductQ"}], "}"}], ",", "\n", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"pandigitalQ", "[", "n_", "]"}], "[", "d_", "]"}], ":=", 
     RowBox[{
      RowBox[{"FromDigits", "@", 
       RowBox[{"Sort", "@", 
        RowBox[{"Flatten", "@", 
         RowBox[{"Thread", "@", 
          RowBox[{"IntegerDigits", "[", 
           RowBox[{"{", 
            RowBox[{"d", ",", 
             RowBox[{"n", "/", "d"}], ",", "n"}], "}"}], "]"}]}]}]}]}], 
      "\[Equal]", "123456789"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pandigitalProductQ", "[", "n_", "]"}], ":=", 
     RowBox[{
      RowBox[{"AnyTrue", "[", 
       RowBox[{"pandigitalQ", "[", "n", "]"}], "]"}], "@", 
      RowBox[{
       RowBox[{"DeleteCases", "[", 
        RowBox[{"1", "|", "n"}], "]"}], "@", 
       RowBox[{"Divisors", "[", "n", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
    
    RowBox[{"Total", "@", 
     RowBox[{
      RowBox[{"Select", "[", "pandigitalProductQ", "]"}], "@", 
      RowBox[{
       RowBox[{"Map", "[", "FromDigits", "]"}], "@", 
       RowBox[{"Permutations", "[", 
