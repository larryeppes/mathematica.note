Cell[CellGroupData[{

Cell["ans 9", "Section",
 CellChangeTimes->{{3.7310609821780577`*^9, 3.7310609834412518`*^9}}],

Cell[TextData[{
 "A Pythagorean triplet is a set of three natural numbers, a < b < c, for \
which,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["a", "2"], "+", 
     SuperscriptBox["b", "2"]}], "=", 
    SuperscriptBox["c", "2"]}], TraditionalForm]]],
 "\nFor example, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["3", "2"], "+", 
     SuperscriptBox["4", "2"]}], "=", 
    RowBox[{
     RowBox[{"9", "+", "16"}], "=", 
     RowBox[{"25", "=", 
      SuperscriptBox["5", "2"]}]}]}], TraditionalForm]]],
 ".\nThere exists exactly one Pythagorean triplet for which a + b + c = 1000.\
\nFind the product ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", " ", "b", " ", "c"}], TraditionalForm]]],
 "."
}], "Text",
 CellChangeTimes->{{3.7312323162559547`*^9, 3.731232373185432*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"res", "=", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"a", "^", "2"}], "+", 
        RowBox[{"b", "^", "2"}]}], "\[Equal]", 
       RowBox[{"c", "^", "2"}]}], "&&", 
      RowBox[{
       RowBox[{"a", "+", "b", "+", "c"}], "\[Equal]", "1000"}], "&&", 
      RowBox[{"a", ">", "0"}], "&&", 
      RowBox[{"b", ">", "a"}], "&&", 
      RowBox[{"c", ">", "0"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"a", ",", "b", ",", "c"}], "}"}], ",", "Integers"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Times", "@@", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "b", ",", "c"}], "}"}], "/.", 
    RowBox[{"res", "[", 
     RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}], "\n", 
 RowBox[{"ClearAll", "[", "res", "]"}]}], "Input",
 CellChangeTimes->{{3.731060995863167*^9, 3.7310610315459538`*^9}, {
