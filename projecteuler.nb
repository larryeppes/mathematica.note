    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "+", "1"}], ")"}], "/", "2"}]}], ")"}], "^", "2"}], "-", 
    RowBox[{"n", 
     RowBox[{"(", 
      RowBox[{"n", "+", "1"}], ")"}], 
     RowBox[{
