   RowBox[{
    RowBox[{
     RowBox[{"Fibonacci", "[", 
      RowBox[{"3", "n"}], "]"}], "\[LessEqual]", "4000000"}], ",", 
    RowBox[{
     RowBox[{"sum", "+=", 
      RowBox[{"Fibonacci", "[", 
       RowBox[{"3", "n"}], "]"}]}], ";", 
     RowBox[{"n", "++"}]}]}], "]"}], 
